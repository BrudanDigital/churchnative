<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/widget32"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@color/black" >

    <LinearLayout
        android:id="@+id/widget86"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:background="@color/black"
        android:orientation="vertical" >

        <RelativeLayout
            android:id="@+id/details_relativeLayout0"
            android:layout_width="fill_parent"
            android:layout_height="30dp" >

            <TextView
                android:id="@+id/details_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:text="Name Of Event"
                android:textColor="@color/white" />
        </RelativeLayout>

        <View
            android:layout_width="fill_parent"
            android:layout_height="2dip"
            android:background="@color/white" />

        <RelativeLayout
            android:id="@+id/details_relativeLayout1"
            android:layout_width="fill_parent"
            android:layout_height="48dp" >

            <TextView
                android:id="@+id/details_descriptionLabel"
                android:layout_width="123dp"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_centerVertical="true"
                android:text="DESCRIPTION"
                android:textColor="@color/orange" />

            <TextView
                android:id="@+id/details_descriptionValue"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBaseline="@+id/details_descriptionLabel"
                android:layout_alignBottom="@+id/details_descriptionLabel"
                android:layout_marginLeft="34dp"
                android:layout_toRightOf="@+id/details_descriptionLabel"
                android:gravity="center"
                android:text="TextView"
                android:textColor="@color/white" />

        </RelativeLayout>

        <View
            android:layout_width="fill_parent"
            android:layout_height="2dip"
            android:background="@color/white" />

        <RelativeLayout
            android:id="@+id/details_relativeLayout2"
            android:layout_width="fill_parent"
            android:layout_height="48dp" >

            <TextView
                android:id="@+id/details_locationLabel"
                android:layout_width="123dp"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_centerVertical="true"
                android:text="LOCATION"
                android:textColor="@color/orange" />

            <TextView
                android:id="@+id/details_locationValue"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBaseline="@+id/details_locationLabel"
                android:layout_alignBottom="@+id/details_locationLabel"
                android:layout_marginLeft="37dp"
                android:layout_toRightOf="@+id/details_locationLabel"
                android:gravity="center"
                android:text="TextView"
                android:textColor="@color/white" />

        </RelativeLayout>

        <View
            android:layout_width="fill_parent"
            android:layout_height="2dip"
            android:background="@color/white" />

        <RelativeLayout
            android:id="@+id/details_relativeLayout3"
            android:layout_width="fill_parent"
            android:layout_height="48dp" >

            <TextView
                android:id="@+id/details_typeLabel"
                android:layout_width="123dp"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_centerVertical="true"
                android:text="TYPE"
                android:textColor="@color/orange" />

            <TextView
                android:id="@+id/details_typeValue"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBaseline="@+id/details_typeLabel"
                android:layout_alignBottom="@+id/details_typeLabel"
                android:layout_marginLeft="40dp"
                android:layout_toRightOf="@+id/details_typeLabel"
                android:gravity="center"
                android:text="TextView"
                android:textColor="@color/white" />

        </RelativeLayout>

        <View
            android:layout_width="fill_parent"
            android:layout_height="2dip"
            android:background="@color/white" />

        <RelativeLayout
            android:id="@+id/details_relativeLayout4"
            android:layout_width="fill_parent"
            android:layout_height="48dp" >

            <TextView
                android:id="@+id/details_dateLabel"
                android:layout_width="123dp"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_centerVertical="true"
                android:text="DATE"
                android:textColor="@color/orange" />

            <TextView
                android:id="@+id/details_dateValue"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBaseline="@+id/details_dateLabel"
                android:layout_alignBottom="@+id/details_dateLabel"
                android:layout_marginLeft="40dp"
                android:layout_toRightOf="@+id/details_dateLabel"
                android:gravity="center"
                android:text="TextView"
                android:textColor="@color/white" />

        </RelativeLayout>

        <View
            android:layout_width="fill_parent"
            android:layout_height="2dip"
            android:background="@color/white" />

        <RelativeLayout
            android:id="@+id/details_relativeLayout5"
            android:layout_width="fill_parent"
            android:layout_height="48dp" >

            <TextView
                android:id="@+id/details_timeLabel"
                android:layout_width="123dp"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_centerVertical="true"
                android:text="START TIME"
                android:textColor="@color/orange" />

            <TextView
                android:id="@+id/details_timeValue"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBaseline="@+id/details_timeLabel"
                android:layout_alignBottom="@+id/details_timeLabel"
                android:layout_marginLeft="40dp"
                android:layout_toRightOf="@+id/details_timeLabel"
                android:gravity="center"
                android:text="TextView"
                android:textColor="@color/white" />

        </RelativeLayout>

        <View
            android:layout_width="fill_parent"
            android:layout_height="2dip"
            android:background="@color/white" />

        <RelativeLayout
            android:id="@+id/details_relativeLayout6"
            android:layout_width="fill_parent"
            android:layout_height="48dp" >

            <TextView
                android:id="@+id/details_durationLabel"
                android:layout_width="123dp"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_centerVertical="true"
                android:text="DURATION"
                android:textColor="@color/orange" />

            <TextView
                android:id="@+id/details_durationValue"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBaseline="@+id/details_durationLabel"
                android:layout_alignBottom="@+id/details_durationLabel"
                android:layout_marginLeft="42dp"
                android:layout_toRightOf="@+id/details_durationLabel"
                android:gravity="center"
                android:text="TextView"
                android:textColor="@color/white" />

        </RelativeLayout>

        <View
            android:layout_width="fill_parent"
            android:layout_height="2dip"
            android:background="@color/white" />

        <RelativeLayout
            android:id="@+id/details_relativeLayout6"
            android:layout_width="fill_parent"
            android:layout_height="80dp" >

            <RatingBar
                android:id="@+id/details_ratingBar"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" 
                android:layout_below="@+id/details_ratingsLabel"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"/>

            <TextView
                android:id="@+id/details_ratingsLabel"
                android:layout_width="123dp"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_centerHorizontal="true"
                android:text="RATE THIS EVENT"
                android:textColor="@color/white" />

        </RelativeLayout>
    </LinearLayout>

</ScrollView>